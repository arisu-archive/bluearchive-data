    {
      "group_id": 8430003010,
      "target_index": 0,
      "dialog_type": "Think",
      "duration": 2500,
      "duration_kr": 0,
      "hide_ui": true,
      "localize_kr": "(아직 할 일이 있어.)",
      "localize_jp": "（まだやることがあってな)",
      "localize_th": "(ยังมีงานที่ต้องทำอยู่)",
