    {
      "group_id": 8431003000,
      "target_index": 0,
      "dialog_type": "Think",
      "duration": 2000,
      "hide_ui": true,
      "localize_kr": "(어디 한 번 해볼까요.)",
      "localize_jp": ""
    },
    {
      "group_id": 8431003001,
      "target_index": 0,
      "dialog_type": "Talk",
      "duration": 2000,
      "hide_ui": true,
